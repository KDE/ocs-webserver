    echo '</div>';
}

if (($this->totalcount > $this->pageLimit) && ($this->page == 1) && ($this->nopage == 0)) {
    $pagehtml = $this->printPaging($this->totalcount, $this->pageLimit, $this->page, '');
    echo $pagehtml;
}
