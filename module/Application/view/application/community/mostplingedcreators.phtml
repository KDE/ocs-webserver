if (($this->page == 1) && ($this->nopage == 0)) {
    echo '<div class="product-list">';
}

$this->nr = ($this->page - 1) * $this->pageLimit + 1;
echo $this->partialLoop('partials/mostplingedcreators.phtml', $this->users);

