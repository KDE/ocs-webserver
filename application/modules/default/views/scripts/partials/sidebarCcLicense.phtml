<script>
    LicenseView = {

        cc_license_data: {
            by:<?=$dataLicense->by == true ? 'true' : 'false'?>,
            nc:<?=$dataLicense->nc == true ? 'true' : 'false'?>,
            nd:<?=$dataLicense->nd == true ? 'true' : 'false'?>,
            sa:<?=$dataLicense->sa == true ? 'true' : 'false'?>
        },

        setup: function () {
            this.setCssLicense();
            this.setLinkLicense();
        },

        setCssLicense: function () {
            css_license = this.buildCssLicense(this.cc_license_data);
            $('.cc-license-btn').attr('class', css_license);
        },

        buildCssLicense: function (obj) {
            var result = "cc-license-btn cc-license-btn";
            for (var i in obj) {
                if (obj.hasOwnProperty(i) && obj[i]) {
                    result += "-" + i;
                }
            }
            return result;
        },

        setLinkLicense: function () {
            $('#cc-license-link').attr('href', this.buildLinkLicense(this.cc_license_data));
        },

        buildLinkLicense: function (obj) {
            var result = "http://creativecommons.org/licenses/";
            for (var i in obj) {
                if (obj.hasOwnProperty(i) && obj[i]) {
                    result += i + "-";
                }
            }
            return result.slice(0, -1) + '/4.0';
        }

    };

    LicenseView.setup();
</script>
<?php endif; ?>